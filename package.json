{
  "name": "audora-ui",
  "version": "0.1.1",
  "title": "Audora UI",
  "description": "An UI design language and React-based implementation",
  "main": "dist/audora-ui.js",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "webpack --config webpack/prod.config.js",
    "build:doc": "styleguidist build",
    "dev": "webpack --config webpack/dev.config.js --watch",
