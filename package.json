{
  "name": "audora-ui",
  "version": "0.1.1",
  "title": "Audora UI",
  "description": "An UI design language and React-based implementation",
  "main": "dist/audora-ui.js",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "webpack --config webpack/prod.config.js",
    "start": "webpack --config webpack/dev.config.js",
    "doc": "styleguidist server",
    "doc:build": "styleguidist build",
    "lint": "eslint src --ext .js,.jsx --ignore-pattern '*.spec.jsx'",
    "test": "jest",
    "test:snap": "jest --updateSnapshot",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "precommit": "npm run lint && npm test"
  },
  "author": "Emerson Laurentino <emersonlaurentino@hotmail.com>",
  "keywords": [
    "audora",
    "react",
    "ui",
    "ui-kit",
    "ui-design",
    "styled-components",
    "react-components",
    "components",
    "javascript",
    "framework",
    "frontend"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/audora/audora-ui"
  },
  "bugs": {
    "url": "https://github.com/audora/audora-ui/issues"
  },
  "homepage": "https://github.com/audora/audora-ui#README",
  "peerDependencies": {
    "prop-types": ">=15",
    "react": ">=16",
    "react-dom": ">=16"
  },
  "dependencies": {
    "color": "^3.0.0",
    "lodash.get": "^4.4.2",
    "lodash.merge": "^4.6.1",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-content-loader": "^3.1.2",
    "react-dom": "^16.2.0",
    "styled-components": "^3.3.2"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.1",
    "eslint": "^4.19.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jest": "^21.15.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.7.0",
    "husky": "^0.14.3",
    "jest": "^22.1.4",
    "prettier": "^1.13.5",
    "react-styleguidist": "^6.2.2",
    "sinon": "^4.2.2",
    "svg-inline-loader": "^0.8.0",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.6",
    "webpack-merge": "^4.1.3"
  },
  "jest": {
    "setupFiles": [
      "./config/test/setup.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  }
}
