{
  "name": "audora-ui",
  "version": "0.0.1",
  "title": "Audora UI",
  "description": "An UI design language and React-based implementation",
  "main": "dist/audora-ui.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rimraf ./dist",
    "build": "npm run clean && cross-env NODE_ENV=production webpack --progress",
    "start": "npm run clean && cross-env NODE_ENV=development webpack --progress --watch",
    "doc": "styleguidist server",
    "doc:build": "styleguidist build",
    "lint": "eslint src --ext .js,.jsx --ignore-pattern '*.spec.jsx'",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prepush": "npm run lint && npm test"
  },
  "author": "Emerson Laurentino <emersonlaurentino@hotmail.com>",
  "keywords": [
    "audora",
    "react",
    "ui",
    "ui-kit",
    "ui-design",
    "styled-components",
    "react-components",
    "components",
    "javascript",
    "framework",
    "frontend"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/audora/audora-ui"
  },
  "bugs": {
    "url": "https://github.com/audora/audora-ui/issues"
  },
  "homepage": "https://github.com/audora/audora-ui#README",
  "peerDependencies": {
    "prop-types": ">=15",
    "react": ">=16",
    "react-dom": ">=16"
  },
  "dependencies": {
    "color": "^3.0.0",
    "lodash.get": "^4.4.2",
    "lodash.merge": "^4.6.1",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "styled-components": "^3.1.6"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "cross-env": "^5.1.3",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.1",
    "eslint": "^4.17.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.6.1",
    "husky": "^0.14.3",
    "jest": "^22.1.4",
    "react-styleguidist": "^6.2.2",
    "rimraf": "^2.6.2",
    "sinon": "^4.2.2",
    "svg-inline-loader": "^0.8.0",
    "webpack": "^3.10.0"
  },
  "jest": {
    "setupFiles": [
      "./scripts/test/setup.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  }
}
